{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "snt-obt/data-visualisation",
  "title": "Data Visualisation",
  "icon": "lightbulb",
  "category": "widgets",
  "description": "A block that draws a graph from a Google spreadsheet. The tutorial is at https://www.youtube.com/watch?v=DzN5Wqtq5h8",
  "keywords": ["graph", "test"],
  "textdomain": "sntObt",
  "attributes": {
    "sheetUrl": {
      "type": "string",
      "default": ""
    },
    "column": {
      "type": "string",
      "default": ""
    },
    "numberOfRows": {
      "type": "integer",
      "default": 1000
    },
    "barHeight": {
      "type": "integer",
      "default": 0
    },
    "barGap": {
      "type": "integer",
      "default": 0
    }
  },
  "editorScript": "snt-obt-data-visualisation-script"
}
